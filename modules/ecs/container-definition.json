[
    {
        "name": "${name_prefix}-weather-app",
        "image": "${ecr-repo-uri}",
        "portMappings": [
            {
                "protocol": "tcp",
                "containerPort": "${port}"
            }
        ],
        "memory": "${memory}",
        "cpu": "${cpu}",
        "requiresCompatibilities": [
        "FARGATE"
        ],
        "networkMode": "awsvpc",
        "executionRoleArn": "${execution-role-arn}"
    }
]
[
    {
        "name": "${name_prefix}-weather-app",
        "image": "${ecr-repo-uri}",
        "portMappings": [
            {
                "protocol": "tcp",
                "containerPort": 3000
            }
        ],
        "memory": 512,
        "cpu": 256,
        "requiresCompatibilities": [
        "FARGATE"
        ],
        "networkMode": "awsvpc",
        "executionRoleArn": "${execution-role-arn}"
    }
]